{
  "name": "eslint-config-vue2",
  "version": "1.0.0",
  "description": "A configuration of ESLint for Vue 2.x",
  "scripts": {
    "lint:js": "eslint . --ext .js,.vue",
    "lint:js:fix": "eslint . --ext .js,.vue --fix",
    "format:js": "prettier . -c ",
    "format:js:fix": "prettier . --write"
  },
  "author": "Masaki Yamanaka",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-vue": "^7.9.0",
    "prettier": "^2.6.1"
  },
  "dependencies": {}
}
